{
    "Jwt": {
        "Issuer": "https://render.com/"
    },

    "ConnectionStrings": {
        "DefaultConnection": ""
    },

    "Logging": {
        "LogLevel": {
            "Default": "Warning",
            "Microsoft.AspNetCore": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },

    "AllowedHosts": "*",

    "Smtp": {
        "Host": "%SMTP_HOST%",
        "Port": "%SMTP_PORT%",
        "Username": "%SMTP_USERNAME%",
        "Password": "%SMTP_PASSWORD%",
        "From": "%SMTP_FROM%"
    }
}